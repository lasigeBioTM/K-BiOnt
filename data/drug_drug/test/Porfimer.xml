    <sentence id="DDI-DrugBank.d614.s7" text="Preclinical data also suggest that tissue ischemia, allopurinol, calcium channel blockers and some prostaglandin synthesis inhibitors could interfere with PHOTOFRIN    PDT. ">
        <entity id="DDI-DrugBank.d614.s7.e0" charOffset="52-62"
            type="drug" text="allopurinol"/>
        <entity id="DDI-DrugBank.d614.s7.e1" charOffset="65-88"
