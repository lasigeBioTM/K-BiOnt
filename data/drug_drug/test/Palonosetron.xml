    <sentence id="DDI-DrugBank.d768.s0" text="Palonosetron is eliminated from the body through both renal excretion and metabolic pathways with the latter mediated via multiple CYP enzymes. ">
        <entity id="DDI-DrugBank.d768.s0.e0" charOffset="0-11"
            type="drug" text="Palonosetron"/>
    </sentence>
    <sentence id="DDI-DrugBank.d768.s1" text="In vitro studies indicated that palonosetron is not an inhibitor of CYP1A2, CYP2A6, CYP2B6, CYP2C9, CYP2D6, CYP2E1 and CYP3A4/5 (CYP2C19 was not investigated) nor does it induce the activity of CYP1A2, CYP2D6, or CYP3A4/5. ">
        <entity id="DDI-DrugBank.d768.s1.e0" charOffset="32-43"
            type="drug" text="palonosetron"/>
    </sentence>
    <sentence id="DDI-DrugBank.d768.s2" text="Therefore, the potential for clinically significant drug interactions with palonosetron appears to be low. ">
        <entity id="DDI-DrugBank.d768.s2.e0" charOffset="75-86"
            type="drug" text="palonosetron"/>
    </sentence>
    <sentence id="DDI-DrugBank.d768.s3" text="A study in healthy volunteers involving single-dose IV palonosetron (0.75 mg) and steady state oral metoclopramide (10 mg four times daily) demonstrated no significant pharmacokinetic interaction. ">
        <entity id="DDI-DrugBank.d768.s3.e0" charOffset="55-66"
            type="drug" text="palonosetron"/>
        <entity id="DDI-DrugBank.d768.s3.e1" charOffset="100-113"
            type="drug" text="metoclopramide"/>
        <pair id="DDI-DrugBank.d768.s3.p0" e1="DDI-DrugBank.d768.s3.e0"
            e2="DDI-DrugBank.d768.s3.e1" ddi="false"/>
    </sentence>
