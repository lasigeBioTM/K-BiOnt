    <sentence id="DDI-MedLine.d212.s0" text="Improvement of colchicine oral bioavailability by incorporating eugenol in the nanoemulsion as an oil excipient and enhancer.&#xd;&#xa;">
        <entity id="DDI-MedLine.d212.s0.e0" charOffset="15-24"
            type="drug" text="colchicine"/>
